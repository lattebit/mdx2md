{
  "repositories": {
    "shadcn-ui": {
      "url": "https://github.com/shadcn-ui/ui",
      "branch": "main",
      "docsPath": "apps/www/content/docs",
      "configFile": "shadcn-ui.ts",
      "outputPath": "output/shadcn-ui",
      "preset": "fumadocs"
    },
    "nextjs": {
      "url": "https://github.com/vercel/next.js",
      "branch": "canary",
      "docsPath": "docs",
      "configFile": "nextjs.ts",
      "outputPath": "output/nextjs",
      "preset": "fumadocs"
    },
    "hono": {
      "url": "https://github.com/honojs/website",
      "branch": "main",
      "docsPath": ".",
      "outputPath": "output/hono",
      "preset": "vitepress"
    },
    "trpc": {
      "url": "https://github.com/trpc/trpc",
      "branch": "main",
      "docsPath": "www",
      "outputPath": "output/trpc",
      "preset": "docusaurus"
    },
    "prisma": {
      "url": "https://github.com/prisma/docs",
      "branch": "main",
      "docsPath": ".",
      "outputPath": "output/prisma",
      "preset": "docusaurus"
    },
    "fastapi": {
      "url": "https://github.com/fastapi/fastapi",
      "branch": "master",
      "docsPath": "docs/en",
      "configFile": "fastapi.ts",
      "outputPath": "output/fastapi",
      "preset": "mkdocs"
    }
  }
}